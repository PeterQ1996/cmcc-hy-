$(function(){
	$(".newsBox").slide({
		mainCell:".bd ul",
		autoPage:true,
		effect:"leftMarquee",
		autoPlay:true,
		trigger:"click",
		interTime:50
	});
	$('.tabBox').hide();
		$('.tabBox').eq(0).show();	
		$('.tabUl li a').hover(function(){
			var ind=$(this).parent('li').index();
			$(this).parent('li').addClass('on').siblings().removeClass('on');
			$('.tabBox').hide();		
			$('.tabBox:eq('+ind+')').show();
			return false;
	});		
})
</script>
