        $(document).scroll(function() {
					if($(this).scrollTop() > 10) {						
						$("#gotop").css('display','block'); 
					} else {
						$("#gotop").hide();						
					}
				})
				
				//回到顶部
				$("#gotop").click(function() {
					$('body,html').animate({
						scrollTop: 0
					}, 500);
					$(this).hide();
					return false;
				});
				
				$("#btn_submit").click(function()
				{
					strName=$("#nickName").val();
					strLinkType=$("#linkType").val();
					strLinkNumber=$("#linkNumber").val();
					strAdvice=$("#advice").val();
					if(strName.trim()=="")
					{
							alert("请填写您的称呼！");
							return;
					}
					if(strLinkNumber.trim()=="")
					{
							alert("请填写您的联系方式！");
							return;
					}
					if(strAdvice.trim()=="")
					{
							alert("请填写您的宝贵意见！");
							return;
					}
					var o={nickName:strName, linkType:strLinkType, linkNumber:strLinkNumber, advice:strAdvice};
					$.ajax({url:"/pc/myadvice", type:"GET",data:o, success:function(e){$('#myModal').modal('hide');} });
					//requestJson('/pc/myadvice?nickName='+strName+'&playType=gewei', 1, null);
				})
