<script type="text/javascript">
	//二级导航
	jQuery(".nav").slide({
		type: "menu",
		titCell: ".nLi",
		targetCell: ".subNavi",
		effect: "slideDown",
		delayTime: 300,
		triggerTime: 0,
		returnDefault: true
	});

	$(window).scroll(function() {
		var dis = $(window).scrollTop();
		if(dis >= 100) {
			$('.login-bar').show();
			$('.index-header').addClass('nav_fixed')
		} else {
			$('.login-bar').hide();
			$('.index-header').removeClass('nav_fixed')
		}
	});
</script>
