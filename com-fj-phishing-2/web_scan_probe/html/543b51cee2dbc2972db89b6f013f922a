<script language="javascript">
	$(function() {
		$(".close").on("click", function() {
			$(this).parent().hide();
		})
	})
	//左下角轮播
	$(".slideBox1").slide({
		mainCell: ".bd1 ul",
		effect: "leftLoop",
		autoPlay: true
	});

	var left_top = 140,
		right_top = 140,
		float_list = [];
	$(function() {

		// 廳主自改 - 浮動圖
		$(".floatDiv").each(function(i) {
			float_list[i] = $(this);
		});
		for(var i in float_list) {
			var self = float_list[i];

			var picfloat = (self.attr('picfloat') == 'right') ? 1 : 0;
			self.show().Float({
				'floatRight': picfloat,
				'topSide': ((picfloat == 1) ? right_top : left_top)
			});

			// ie6 png bug
			if(navigator.userAgent.toLowerCase().indexOf('msie 6') != -1) {
				$.each(self.find('span'), function() {
					$(this).css({
						'width': $(this).width(),
						'height': $(this).height()
					});
				});
			}

			if(picfloat) {
				right_top = right_top + (self.find('a > span').height() || 300);
			} else {
				left_top = left_top + (self.find('a > span').height() || 300);
			}

			self.find('.floatBox div').click(function() {
				event.cancelBubble = true;
				$(this).parent('.floatBox').hide();
			});
		}
		$(".kf-content-left").mouseover(function() {
			if($(".kf-content-right").width() == 0) {
				kfAnimate(".kf-content-right", "152px");
			} else {
				kfAnimate(".kf-content-right", "0");
			}
		});

		setInterval("kfAnimate('.kf-content-right','0')", 35000);

		$('.kf-content-right').css('width', '0px');

	});

	function kfAnimate(cls, data) {
		$(cls).animate({
			width: data
		}, {
			queue: false,
			duration: 150
		});
	}
</script>

