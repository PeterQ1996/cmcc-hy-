		function lunbo() {
			$.ajax({
				url : "/getConfig/getLunBo.do",
				type : "GET",
				dataType : "json",
				success : function(j) {
					var col = '';
					var dian = '';
					imgsize = j.length;
					for (var i = 0; i < imgsize; i++) {
						col += '<li><a><img src="'+j[i].titleImg+'" /></a></li>'
						dian += '<li></li>';
					}
					$("#lunbo").html(col);
					$("div.hd ul").html(dian);
					jQuery(".slideBox").slide({mainCell:".bd ul",autoPlay:true});
				}
			});
		}
		
		lunbo();
		
		</script>

					


