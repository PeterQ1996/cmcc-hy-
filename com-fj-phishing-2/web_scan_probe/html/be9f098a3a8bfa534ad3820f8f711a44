 function gameScore(e){if (sessionStorage.is_login !="true") {loginObj.getLoginPopup();}else{var gameId =$(e).data("game-id");var score =$(e).data("score");$.ajax({url:"/siteGame/updateGameScore.html",dataType:"JSON",type:'POST',data:{"result.gameId":gameId,"result.score":score},success:function(data) {alert(data.msg);},error:function (data) {alert(data.msg);}
