}
});}
 function closeIframeLayerDialog(content,title,skin,area,btnRound,btnBorder){layer.open({type:1,content:content,title:title,skin:skin,area:area,btn:["确定"],yes:function (index) {layer.close(index);$("html").removeClass("game-detail-open");},success:function(layer){$(layer).find('.layui-layer-setwin').html('<a class="layui-layer-close" href="javascript:;">	&times;</a>');$(layer).addClass("normal-dialog");if(!!btnRound){$(layer).addClass("dialog-btn-round");}
if(!!btnBorder){$(layer).addClass("dialog-btn-border");}
},cancel:function (index) {layer.close(index);$("html").removeClass("game-detail-open");}
});}
function layerDialogIndex(imgSrc,title,skin,area,transparent,btnAlign,txtDialog){var move ='.layui-layer-title';var content ='';if(!!txtDialog){content =imgSrc;layer.open({type:1,content:content,title:title,move:move,btnAlign:btnAlign,skin:skin,area:area,btn:["关闭"],success:function(layer){$(layer).find('.layui-layer-setwin').html('<a class="layui-layer-close" href="javascript:;">	&times;</a>');$(layer).addClass("index-modal ");if(!!transparent){$(layer).addClass("index-modal-transparent");}
},cancel:function(){if($("#home-dialog-checkbox").is(":checked")){localStorage.setItem("10072-close-home-dialog",true);}
},yes:function(){if($("#home-dialog-checkbox").is(":checked")){localStorage.setItem("10072-close-home-dialog",true);}
layer.closeAll();}
});}else{var src =imgSrc;var img =new Image();img.src =src;img.addEventListener('load',function () {var _href ="";if(_href!=undefined &&_href!=""){if(_href.indexOf("http")>-1){_href =_href;}else{_href ="http://"+_href;}
if(_href.indexOf("\$\{website\}")>-1){_href =_href.replace("\$\{website\}",window.location.host);}
}else{_href ="javascript:void(0)";}
img =$("<a href='"+_href+"'></a>").append(img);if(!!transparent){content =$("<div></div>").append($(img));}else{content =$("<div></div>").append($(img)).append('<div class="checkbox-wrap"><input type="checkbox" id="home-dialog-checkbox" >关闭后，不再显示本弹窗广告 <div>');}
layer.open({type:1,content:content.html(),title:title,move:move,btnAlign:btnAlign,skin:skin,area:area,btn:["关闭"],success:function(layer){$(layer).find('.layui-layer-setwin').html('<a class="layui-layer-close" href="javascript:;">	&times;</a>');$(layer).addClass("index-modal ");if(!!transparent){$(layer).addClass("index-modal-transparent");}
},cancel:function(){if($("#home-dialog-checkbox").is(":checked")){localStorage.setItem("10072-close-home-dialog",true);}
},yes:function(){if($("#home-dialog-checkbox").is(":checked")){localStorage.setItem("10072-close-home-dialog",true);}
layer.closeAll();}
});},false);}
}
