});});function layerDialogNormal(content,title,skin,area,btnRound,btnBorder,createAccount,closeWindow){layer.open({type:1,content:content,title:title,skin:skin,area:area,btn:["确定"],success:function(layer){$(layer).find('.layui-layer-setwin').html('<a class="layui-layer-close" href="javascript:;">	&times;</a>');$(layer).addClass("normal-dialog");if(!!btnRound){$(layer).addClass("dialog-btn-round");}
