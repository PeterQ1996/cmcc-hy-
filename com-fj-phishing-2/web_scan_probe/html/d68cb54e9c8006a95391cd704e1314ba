        <script>
            time('#time1', 23, 80);
            time('#time2', 122, 15);
            function time(el, end, speed) {
                var start = $(el).text();
                var timer;
                if (start.length > 1) {//显示时间进度
                    var html;
                    var min = 0;
                    var sec = 0;
                    var num = 0;
                    clearInterval(timer);
                    timer = setInterval(function () {
                        sec++;
                        num++;
                        sec = sec < 10 ? '0' + sec : sec;
                        sec < 60 ? min : min++;
                        sec = sec < 60 ? sec : 0;
                        sec = sec == 0 ? '0' + sec : sec;
                        html = min + '‘' + sec;
                        $(el).text(html);
                        if (num >= end) {
                            clearInterval(timer);
                        }
                    }, speed)
                } else {
                    clearInterval(timer);
                    timer = setInterval(function () {//显示普通进度
                        start++;
                        $(el).text(start);
                        if (start >= end) {
                            clearInterval(timer);
                        }
                    }, speed)
                }
            }

        </script>
