                function random(id, num, base) {
                    var obj = document.getElementById(id);
                    var num2;
                    num2 = base < 0 ? 10 : 0;
                    setInterval(function () {
                        num += parseInt(Math.random() * base) + num2;
                        obj.innerHTML = num;
                    }, 2600);
                }
                random('body', 14598, -15);
                random('bet', 29350679, 50);
                random('withdraw', 50622, 10);
