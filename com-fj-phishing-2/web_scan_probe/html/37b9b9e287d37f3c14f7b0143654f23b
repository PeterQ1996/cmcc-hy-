<script>
    function login_submit() {
        var tip_title=document.getElementById('tip_title');
        var tip_info=document.getElementById('tip_info');
        tip_title.innerHTML='登录中';
        tip_info.innerHTML='<div class="mdui-spinner" id="spinner"></div>&nbsp;登录中';
        mdui.updateSpinners(document.getElementById('spinner'));
        document.getElementById('tip_button').disabled=true;
        var ajax;
        var username=document.getElementById('username').value;
        var password=document.getElementById('password').value;
        ajax=new XMLHttpRequest();
        ajax.open('POST','ajax/user.php?action=login',true);
        ajax.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
        ajax.send('username=' + username + '&password=' + password);
        ajax.onreadystatechange=function () {
            if (ajax.readyState==4&&ajax.status==200){
                var result=JSON.parse(ajax.responseText);
                tip_info.innerHTML=result['data']['message'];
                if (result['code']==103||result['code']==102||result['code']==104){
                    tip_title.innerHTML="登录失败";
                }
                if (result['code']==201){
                    tip_title.innerHTML="登录成功";
                    window.location="index.php";
                }
                document.getElementById('tip_button').disabled=false;
            }
        }
    }
</script>
