        $(function () {
            var kefuw = $(".J-kefuw-fixed");
            var kefuw0 = kefuw.eq(0);
            var height = kefuw0.height();

            $(window).scroll(function (evt) {
                var top = $(document).scrollTop();
                if (kefuw0.position().top + height < top - 40) {
                    kefuw.css("top", top - 40 + "px");
                }

                kefuw.stop().animate({ top: 240 + top + "px" }, 500, "linear");
            });
        })
