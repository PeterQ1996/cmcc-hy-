        <script type="text/javascript">
            var tt=Math.random();
            $.cookie('gotopc','true',{path: '/','expires':1});
            if (typeof($.cookie('tt')) == 'undefined' || $.cookie('tt') == null) {
            $.cookie('tt', tt);//设置
            }
            var time=$.cookie('tt');
            seajs.use(['/public/js/main/index.js?i=1'+time], function (){
            seajs.use(['config','cookie','layer'], function (){//在线客服和在线qq
                objClassStr.str_service_qq = "";
                objClassStr.str_service_online = "";
                var page_status = $.cookie(objClassStr.str_page_key);
                switch (page_status) {//根据当前页cookie加载中间部分，刷新还在当前页
                    case objClassStr.str_for_page_index:
                        $(".th_index").parent(".nLi").addClass("on1");
                        break;
                    case objClassStr.str_for_page_live:
                         $(".th_live").parent(".nLi").addClass("on1");
                        break;
                    case objClassStr.str_for_page_promos:
                         $(".th_promos").parent(".nLi").addClass("on1");
                        break;
                    case objClassStr.str_for_page_sport:
                         $(".th_sport").parent(".nLi").addClass("on1");
                        break;
                    case objClassStr.str_for_page_dsgame:
                         $(".th_dsgame").parent(".nLi").addClass("on1");
                        break;
                    default:
                         $(".th_index").parent(".nLi").addClass("on1");
                        break;
                   }
            });
            });
            $(".sub").hover(function(){
                $(this).parent(".nLi").addClass("on2");
            },
            function(){
                $(this).parent(".nLi").removeClass("on2");
            });
            $(".nLi").click(function(){
                $(this).parent(".nav-list").children().removeClass("on1");
                $(this).addClass("on1");
            });
            $(function(){
            	var myDate = new Date();
            	var years = myDate.getFullYear();
            	$('#year-local').append(years);						
            });
            function SetHome(url){
                if (document.all) {
                    document.body.style.behavior='url(#default#homepage)';
                    document.body.setHomePage(url);
                }else{
                    alert("您好,您的浏览器不支持自动设置页面为首页功能，请您手动在浏览器里设置该页面为首页!");
                }
            }
            function AddFavorite(sURL, sTitle) {
                sURL = encodeURI(sURL);
                try {
                    window.external.addFavorite(sURL, sTitle);
                } catch (e) {
                    try {
                        window.sidebar.addPanel(sTitle, sURL, "");
                    } catch (e) {
                        alert("放进收藏失败，请使用Ctrl+D进行添加，或手动在浏览器里进行设置.");
                    }
                }
            }
        $(".welcome a").click(function(){
            $(".welcome").css("display","none");
        })
        </script>
