<script type="text/javascript">
$(document).ready(function(){
    var l = true;
    var top = $("#frfix1").offset().top;
    function doFix()
    {              
        var scrolla=$(window).scrollTop();
        var dis=parseInt(top)-parseInt( scrolla);
        var dis1=parseInt(top)-parseInt( scrolla)+300;
        $("#frfix1").attr("kep",dis);
        if(dis1<-940){
            $("#frfix1").css("top",dis1+940+'px');
            $("#frfix2").css("top",dis1+940+70+'px');
            $("#frfix3").css("top",dis1+940+400+'px');
        }else{
            $("#frfix1").css("top",'0px');
            $("#frfix2").css("top",'70px');
            $("#frfix3").css("top",'400px');
        }
        if(l && dis<=0)
        {                
            $("#frfix1").addClass("fixed_right");
            $("#frfix2").addClass("fixed_right2");
            $("#frfix3").addClass("fixed_right3");
            l=false;
        }
        if(!l && dis>0)
        {
            $("#frfix1").removeClass("fixed_right");
            $("#frfix2").removeClass("fixed_right2");
            $("#frfix3").removeClass("fixed_right3");
            l=true;
        }
    }
    $(window).scroll(doFix);
	
});

</script>


