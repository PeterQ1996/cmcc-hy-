<script type="text/javascript">
    var linkCode = GetQueryString("ad") || "";
    var browser = {
        versions: function() {
            var u = navigator.userAgent, app = navigator.appVersion;
            return {
//移动终端浏览器版本信息
                trident: u.indexOf('Trident') > -1, //IE内核
                presto: u.indexOf('Presto') > -1, //opera内核
                webKit: u.indexOf('AppleWebKit') > -1, //苹果、谷歌内核
                gecko: u.indexOf('Gecko') > -1 && u.indexOf('KHTML') == -1, //火狐内核
                mobile: !!u.match(/AppleWebKit.*Mobile/i) || !!u.match(/MIDP|SymbianOS|NOKIA|SAMSUNG|LG|NEC|TCL|Alcatel|BIRD|DBTEL|Dopod|PHILIPS|HAIER|LENOVO|MOT-|Nokia|SonyEricsson|SIE-|Amoi|ZTE/), //是否为移动终端
                ios: !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/), //ios终端
                android: u.indexOf('Android') > -1 || u.indexOf('Linux') > -1, //android终端或者uc浏览器
                iPhone: u.indexOf('iPhone') > -1 || u.indexOf('Mac') > -1, //是否为iPhone或者QQHD浏览器
                iPad: u.indexOf('iPad') > -1, //是否iPad
                webApp: u.indexOf('Safari') == -1 //是否web应该程序，没有头部与底部
            };
        }(),
        language: (navigator.browserLanguage || navigator.language).toLowerCase()
    }

    //if  (document.referrer.indexOf("google.com")  >  -1  ||  document.referrer.indexOf("baidu.com")  >  -1)  {
    //                alert("从google或baidu来");
    //}
    //alert(document.referrer);
    if (document.referrer.length == 0) {
        clearCookie("ebalbeAdvert");
    }

    $(function() {
        $("#GuanGao1").hide();
        $("#GuanGao2").hide();
        $("#GuanGao3").hide();

        var ad = GetQueryString("ad")

        if (ad == "") {
            ad = getCookie("ebalbeAdvert");
        }

        if (ad.length > 0) {
            setCookie("ebalbeAdvert", ad, 7);
            $(".NavOne_Ul").find(".btnZoushi").show();
            $(".NavOne_Ul").find(".btnApp").hide();

            var val = getCookie("ebalbeAdvert");

            if (val == "toutiao") {
                var tr = $("#lhcTd", "table").closest("tr");
                var nTr = tr.next();
                var nTd = nTr.find("td:eq(0)");
                $("#lhcTd", "table").remove();
                tr.append(nTd.clone(true));
                nTd.remove();

                var url = "https://gsw-public.oss-cn-shenzhen.aliyuncs.com/client/android/caipiaojihua_toutiao_3.2.1.apk";
                $(".Opic").find("a").attr("href", url).click(function() {
                    //alert(0)
                    _taq.push({ convert_id: "1603878034128916", event_type: "download_start" })
                });
                $(".Opic").show();
                $(".NavOne_Ul").find(".btnZoushi").hide();
                $(".NavOne_Ul").find(".btnApp").show().find("a").attr("href", url).click(function() {
                    //alert(0)
                    _taq.push({ convert_id: "1603878034128916", event_type: "download_start" })
                });
                $("#footer_appdown").attr("href", url).click(function() {
                    // alert(0)
                    _taq.push({ convert_id: "1603878034128916", event_type: "download_start" })
                });;


                (function(root) {
                    root._tt_config = true;
                    var ta = document.createElement('script');
                    ta.type = 'text/javascript';
                    ta.async = true;
                    ta.src = document.location.protocol + '//' + 's1.pstatp.com/bytecom/resource/track_log/src/toutiao-track-log.js';
                    ta.onerror = function() {
                        var request = new XMLHttpRequest();
                        var web_url = window.encodeURIComponent(window.location.href);
                        var js_url = ta.src;
                        var url = '//ad.toutiao.com/link_monitor/cdn_failed?web_url=' + web_url + '&js_url=' + js_url + '&convert_id=1603878034128916';
                        request.open('GET', url, true);
                        request.send(null);
                    }
                    var s = document.getElementsByTagName('script')[0];
                    s.parentNode.insertBefore(ta, s);
                })(window);


            } else if (val == "qq") {
