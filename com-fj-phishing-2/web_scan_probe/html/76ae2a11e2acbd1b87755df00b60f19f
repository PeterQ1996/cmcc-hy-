<script type="text/javascript">
    $(window).scroll(function () {
        var sc = $(window).scrollTop();
        $(".rightdao").stop().animate({
            top: sc + 100
        }, 400);
    });
    $(".closedao").click(function () {
        $(this).parent().hide();
    });

    function lunTopFn(obj, time) {
        time = time ? time : 30;
        $(obj).children('ul').append($(obj).children('ul').children().clone(true))
        var len = $(obj).children('ul').height() / 2;
        var times = null;
        if ($(obj)[0]) {
            times = window.setInterval(function () {
                var t = $(obj).children('ul').css('top');
                t = t.replace('px', '');
                if (t > -len) {
                    t--;
                    $(obj).children('ul').css({top: t})
                } else {
                    $(obj).children('ul').css({top: 0})
                }
            }, time)
            $(obj).children('ul').hover(function () {
                window.clearInterval(times);
            }, function () {
                times = window.setInterval(function () {
                    var t = $(obj).children('ul').css('top');
                    t = t.replace('px', '');
                    if (t > -len) {
                        t--;
                        $(obj).children('ul').css({top: t})
                    } else {
                        $(obj).children('ul').css({top: 0})
                    }
                }, time)
            })
        }
    }
    lunTopFn('.luntop', 50);

    // 彈窗
    $(function () {
        $(".link").on('click', function () {
            var tag = $(this).attr("data-tag");
            $(".jp-box").show();
            $(".tag").hide();
            $("." + tag).show();
            return false;
        });
        $(".closebtn").on('click', function () {
            $(".jp-box").hide();
            return false;
        });
    });

    // 登錄
    $('#btn-submit').click(function () {
        $('form').submit()
    });

    // 遊客
    $('#btn-guest').click(function () {
        var obj = $(this);
        obj.attr("disabled", true);
        $.ajax({
            type       : "get",
            contentType: "application/json; charset=utf-8",
            dataType   : "json",
            url        : "/?a=guest",  //这里是网址
            success    : function (data) {
                if (data.code == 0) {
                    alert('注册游客成功!');
                    window.location.href = "/pc.php?c=pc";
                } else {
                    obj.attr("disabled", false);
                    alert('注册游客失败，请重试');
                    return false;
                }
            },
            timeout    : 10000, // 10秒
            error      : function (XMLHttpRequest, textStatus, errorThrown) {
                obj.attr("disabled", false);
                alert('网络异常，请重试');
                return false;
            }
        });
        return false;
    });
</script>
<script type="text/javascript">
    $(function () {
        $.fn.extend({
            'autoQrcode': function (option) {
                var default_settings = {
                    render: "canvas",
                    ecLevel: 'H',//识别度
                    fill: '#000',//二维码颜色
                    background: '#ffffff',//背景颜色
                    quiet: 2,//边距
                    text: $(this).data('url'), // 二维码内容

                    // 中间logo end
                    label: 'jQuery.qrcode',
                    fontname: 'Ubuntu',
                    fontcolor: '#ff9818',
                };
                option = $.extend(default_settings, option);
                $(this).qrcode(option);
            }
        });
        var option = {};

        function renderQrcode(option) {
            $('.qrcode_render').each(function (i, item) {
                var width, height = 0;
                $(item).autoQrcode(option);
                width = $(item).data('width') ? $(item).data('width') : 100;
                height = $(item).data('height') ? $(item).data('height') : 100;
                $(item).find('canvas').width(width).height(height);
            });
        }

        try {
            var img = new Image();
            img.src = "/pc201702/logo/icon/200000074.png";
            img.onload = function () {
                option = {
                    mode: 4,
                    imagesize: .18,
                    image: img,
                };
                renderQrcode(option);
            };
            img.onerror = function () {
                console.log("图标加载失败");
                renderQrcode(option);
            };
        } catch (e) {
            console.log("图标不存在");
            renderQrcode(option);
        }
    });
</script>

