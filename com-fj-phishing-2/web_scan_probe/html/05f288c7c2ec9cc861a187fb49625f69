        function changeCaptcha () {
            $('#J-vcode-img').attr('src', "http://bomao8.com/captcha?" + ((Math.random()*9 +1)*100000).toFixed(0));
        };

        $(function(){
            $('#J-button-submit').click(function (e) {
                $('#J-button-submit .btn-pic').show();

                var pwd = $('#login-pass').val();
                var username = ($('#login-name').val()).toLowerCase();
                $('#login-pass-real').val(md5(md5(md5(username + pwd))));
                $('form[name=signinForm]').submit();
            });

            $('form[name=signinForm]').keydown(function(event) {
                if (event.keyCode == 13) $('#J-button-submit').click();
            });

//            $('body').keydown(function(event) {
//                if (event.keyCode == 13) {
//                    $('.top-slide').addClass('top-slide-c');
//                    $('.top-wrap').addClass('top-wrap-c');
//                }
//            });

            $('.form-login input').bind('input propertychange', function() {
                if(!$('.remind').hasClass('hidden')){
                    $('.remind').addClass('hidden');
                }
            });
        });
    </script>
<script src="/assets/js/login/footer-online-count.js"></script>
<script type="text/javascript">
        $(function(){
            var msg_id = "";
            //提示组件
            var sideTip = bomao.SideTip.getInstance({
                //面板元素dom
                dom:'#J-panel-sidetip-sys',
                //页面主体内容参照元素，面板将以该元素为参照定位
                target:'body',
                //标题元素
                titleDom:'.sidetip-title-text-sys',
                //主体内容元素
                contentDom:'.sidetip-content-sys',
                //关闭面板元素
                closeDom:'.sidetip-close-sys',
                //是否自动关闭
                autoHideTime:20 * 1000
            });

            //修改信息状态
            function changeTipStatus(id) {
                var url = '/receive-promo/'+id;

                $.ajax({
                    url: url,
                    dataType: 'JSON',
                    method: 'GET',
                    cache: false,
                    success: function(data){
                        if(data['status'] == 1){
                            $(".tip-button-sys").attr("btn-status","1");
                            $(".tip-button-sys").html("领取成功");

                            setTimeout(function(){
                                sideTip.hide();
                            },2*1000);
                        }else{
                            sideTip.hide();

                            var tip_window = new bomao.Message();

                            var tip_window_data = {
                                title: '温馨提示',
                                content: "<div style='font-size:16px'><p style='text-align: center;margin:10px 0px;'>领取失败，请在站内信中重新领取</p></div>",
                                isShowMask: true,
                                confirmIsShow: false,
                                closeIsShow: true,
                                closeButtonText: '关闭',
                                closeFun: function () {
                                    this.hide();
                                }
                            };

                            tip_window.show(tip_window_data);
                        }
                    },
                    error:function(data){
                        console.log(data);
                    }
                });
            };

            //查询提示信息
            var MSG = new bomao.Alive({
                url: "/get-messages",
                cache:false,
                dataType:'json',
                method:'get',
                looptime:30 * 1000
            });

            MSG.addEvent('afterSuccess', function(e, data){
                if(!(data.length == 0) && data['type']!=="loginTimeout"){
                    msg_id = data["id"];
                    sideTip.hide();
                    if(Number(data["amount"]) > 0){
                        sideTip.setContent('<div class="row">'+data["content"]+'</div><div class="tip-button-sys" btn-status="-1">领取</div>');
                    }else{
                        sideTip.setContent('<div class="row">'+data["content"]+'</div>');
                    }
                    sideTip.show();
                }

                $("#J-panel-sidetip-sys .tip-button-sys").on("click" , function(){
                    if($(this).attr("btn-status") == "-1"){
                        changeTipStatus(msg_id);
                    }
                });
            });
            MSG.addEvent('afterError', function(e, xhr, type){
                console.log("error");
            });

            MSG.getParams = function(){
                var params = [];
                return {'params':params};
            };

            MSG.start();

        });

        $(function($) {
            //页脚微信的hover效果
            $('.foot-icon li').hover(function () {
                $(this).find('.up-drop').stop(true, false).fadeIn(400);
                $(this).find('.up-menu').addClass('up-menu-hov');
            }, function () {
                $(this).find('.up-drop').stop(true, false).fadeOut(400);
                $(this).find('.up-menu').removeClass('up-menu-hov');
            });

            //回到顶部
            var refer = $('#J-header-container'),dom,offset,win,timer;
            if(refer.size() < 1){
                //首页应用
                $("#J-global-gototop").click(function(e){
                    e.preventDefault();
                    $('html,body').animate({scrollTop:0}, 400);
                });

                setTimeout(function(){
                    $(window).scroll(function(){
                        clearTimeout(timer);
                        timer = setTimeout(function(){
                            if($(window).scrollTop() > 600){
                                $("#J-global-gototop").fadeIn(400);
                            }else{
                                $("#J-global-gototop").fadeOut(400);
                            }
                        }, 300);
                    });
                });

                return;
            }
            win = $(window);
            offset = refer.offset();

            setTimeout(function(){
                dom = $('<a class="global-gototop" id="J-global-gototop" href="#">返回顶部</a>').appendTo('body');
                dom.css({'left':offset.left + refer.width() + 20});
                dom.click(function(e){
                    e.preventDefault();
                    $('html,body').animate({scrollTop:0}, 400);
                });
                win.scroll(function(){
                    clearTimeout(timer);
                    timer = setTimeout(function(){
                        if(win.scrollTop() > 600){
                            dom.fadeIn(400);
                        }else{
                            dom.fadeOut(400);
                        }
                    }, 300);

                });
            }, 2000);

            var foot = $('#jack-foot'), arr = ['/bets/bet/60', '/bets/bet/61'];
            if(foot.offset().top + foot.height() + 20 < $(window).height() && arr.indexOf( location.href) == -1)
                foot.css('marginTop', ($(window).height() - foot.offset().top - foot.height() - 20) + 'px')

        });

        <!--数字滚动效果-->
        $(function () {

            var numRun1 = $(".numberRun1").numberAnimate({num: 1, speed: 1000, symbol: ","});
            var numRun2 = $(".numberRun2").numberAnimate({num: 1, speed: 1000, symbol: ","});
            // 凌晨0点至0点30分时 数据取不到

            var arr = [84529, 69];

            $.ajax({
                url: '/auth/getonlineuser',
                dataType: 'JSON',
                method: 'POST',
                success: function(data){
                    if(data['prize']){
                        $(".numberRun1").numberAnimate({num: data['prize'], speed: 1000, symbol: ","});
                    }else{
                        $(".numberRun1").numberAnimate({num: arr[0], speed: 1000, symbol: ","});
                    }

                    if(data['online']){
                        $(".numberRun2").numberAnimate({num: data['online'], speed: 1000, symbol: ","});
                    }else{
                        $(".numberRun2").numberAnimate({num: arr[1], speed: 1000, symbol: ","});
                    }
                }
            });


            setInterval(function(){
                $.ajax({
                    url: '/auth/getonlineuser',
                    dataType: 'JSON',
                    method: 'POST',
                    success: function(data){
                        if(data['prize']){
                            numRun1.resetData(data['prize']);
                        }else{
                            numRun1.resetData(arr[0] - parseInt(Math.random()*1000));
                        }

                        if(data['online']){
                            numRun2.resetData(data['online']);
                        }else{
                            numRun2.resetData(arr[1] - parseInt(Math.random()*10));
                        }
                    }
                });
            } , 30*1000);

        });
    </script>
<script type="text/javascript">
        var current_time = "125352";

        $(document).on('keyup', '.feedback-textarea', function(){
            var v = this.value,len = v.length,maxlen = 1000;
            $('#J-text-customer-service-length').text(len);
            if(len > maxlen){
                this.value = v.substr(0, maxlen);
            }
        });

        $(".online-server").on("click" , function(){
                        var url = "https://vp9.live800.com/live800/chatClient/chatbox.jsp?companyID=80000041&configID=2083&codeType=custom&info=("+"userId=&loginname=&name=&hashCode=&amp;timestamp=1533704032" +")";
                window.open(url,"_blank","height=700,width=900");
        });

    </script>


