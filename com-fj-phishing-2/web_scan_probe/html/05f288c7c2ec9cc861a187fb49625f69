<script type="text/javascript">
        (function ($) {
            //ie8浏览器让j-l-content的内容隐藏
            if (navigator.userAgent.indexOf("MSIE 8.0") > 0) {
                document.getElementById('j-l-content').style.display = 'none';
            }

            //判断移动还是pc进入，显示进入移动端的按钮
            if(/Android|webOS|iPhone|iPod|BlackBerry/i.test(navigator.userAgent)) {
                $('#common-btn-pc .btn-text').text('进入PC版');
                $('#common-btn-p').hide();
                $('#common-btn-mobile').show();
                $('.top-slide').addClass('top-slide-0');
            } else {
                $('#common-btn-pc .btn-text').text('进入');
                $('#common-btn-p').show();
                $('#common-btn-mobile').hide();
            }
        })(jQuery);
    </script>
