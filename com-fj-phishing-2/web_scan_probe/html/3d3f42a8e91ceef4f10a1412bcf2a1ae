<script type="text/javascript">
    $(function () {
        $.fn.extend({
            'autoQrcode': function (option) {
                var default_settings = {
                    render: "canvas",
                    ecLevel: 'H',//识别度
                    fill: '#000',//二维码颜色
                    background: '#ffffff',//背景颜色
                    quiet: 2,//边距
                    text: $(this).data('url'), // 二维码内容

                    // 中间logo end
                    label: 'jQuery.qrcode',
                    fontname: 'Ubuntu',
                    fontcolor: '#ff9818',
                };
                option = $.extend(default_settings, option);
                $(this).qrcode(option);
            }
        });
        var option = {};

        function renderQrcode(option) {
            $('.qrcode_render').each(function (i, item) {
                var width, height = 0;
                $(item).autoQrcode(option);
                width = $(item).data('width') ? $(item).data('width') : 100;
                height = $(item).data('height') ? $(item).data('height') : 100;
                $(item).find('canvas').width(width).height(height);
            });
        }

        try {
            var img = new Image();
            img.src = "/pc201702/logo/icon/200000086.png";
            img.onload = function () {
                option = {
                    mode: 4,
                    imagesize: .18,
                    image: img,
                };
                renderQrcode(option);
            };
            img.onerror = function () {
                console.log("图标加载失败");
                renderQrcode(option);
            };
        } catch (e) {
            console.log("图标不存在");
            renderQrcode(option);
        }
    });
</script>

</body></html>