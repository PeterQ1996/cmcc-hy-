                    <script type="text/javascript">
                        function guest() {
                            obj = $(this);
                            obj.attr("disabled", true);
                            $.ajax({
                                type       : "get",
                                contentType: "application/json; charset=utf-8",
                                dataType   : "json",
                                url        : "/?a=guest",  //这里是网址
                                success    : function (data) {
                                    if (data.code == 0) {
                                        alert('注册游客成功!');
                                        window.location.href = "/pc.php?c=pc";
                                    } else {
                                        obj.attr("disabled", false);
                                        alert('注册游客失败，请重试');
                                        return false;
                                    }
                                },
                                timeout    : 10000, // 10秒
                                error      : function (XMLHttpRequest, textStatus, errorThrown) {
                                    obj.attr("disabled", false);
                                    alert('网络异常，请重试');
                                    return false;
                                }
                            });
                            return false;
                        }
                    </script>
