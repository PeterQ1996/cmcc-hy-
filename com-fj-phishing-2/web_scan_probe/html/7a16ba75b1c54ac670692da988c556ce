	$(document).ready(function(){
		$("#maskDiv").height(document.body.scrollHeight);
		$("#maskDiv").width(document.body.scrollWidth);
		var islogin = false
		if (islogin ==true){
		}else{
		 $("#loginMask").height(document.body.scrollHeight);
		/*  $("#loginMask").width(document.body.scrollWidth); */
		 $("#floatLogins div").bind("click",function(event){
