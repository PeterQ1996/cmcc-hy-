<script>

$(document).ready(function(){
	var islogin = false;
	//试玩
	$(".clearfix li .demo").click(function(){
		/* if (islogin == true) {
			JqueryShowMessageJustClose('您已登录游戏!');
		} else {
			loginDemoFormSubmit();
		} */
		onlineService('https://static.meiqia.com/dist/standalone.html?_=t&eid=88112');
	});
	
	$("#tab-lott01 .clearfix .game-pic").click(function(){
		if (islogin == true) {
			playGameLott('LOTTERY_IG','2','0');
		} else {
			location.href = '../app/allLotteryTicket';
		}
	});
	$("#tab-lott02 .clearfix .game-pic").click(function(){
		if (islogin ==true){
			location.href = '../app/lotto';
		}else {
				location.href = '../app/allLotteryTicket';
		};
	});
	$("#tab-lott03 .clearfix .game-pic").click(function(){
		if (islogin ==true){
			playGame('KY_CHESS_GAME');
		
		}else{
			popupDivNew('pop-div','/caiJiaYuanLoginWeb','CN','caiJiaYuan','logo.swf','location');
		}
	});
	$("#tab-lott04 .clearfix .game-pic").click(function(){
		if (islogin ==true){
			/* playGameLott('GFC_PJ'); */
			location.href = '../app/sportsAll';
		}else{
			location.href = '../app/sportsAll';
		}
	});
	$("#tab-lott05 .clearfix .game-pic").click(function(){
		if (islogin ==true){
			location.href = '../app/electronicGameIG';
		}else {
			location.href = '../app/electronicGameIG';
		};
	});
	$("#tab-lott06 .clearfix .game-pic").click(function(){
		if (islogin ==true){
			location.href = '../app/electronicGameBYW';
		}else {
			location.href = '../app/electronicGameBYW';
		};
	});
	
	
	
});
</script>
