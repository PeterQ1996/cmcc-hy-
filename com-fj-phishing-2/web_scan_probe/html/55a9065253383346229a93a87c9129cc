    var mobileUrl = '';
    if (mobileUrl != '') {
        if (/(iphone)|(ipad)|(android)/i.test(navigator.userAgent)) {
            if (!/\?mobile=no/.test(location.search)) {
                location.href = mobileUrl;
            }
        }
    }
