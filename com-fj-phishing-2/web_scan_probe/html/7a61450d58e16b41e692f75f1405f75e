<script type="text/javascript"> 
$(document).ready(function() {
	$("#home").addClass("actives");
	var ind = 0;
	$('.tabSection .tabBox').hide();
	$('.tabSection .tabBox').eq(0).show();

	$('.tabSection .tabUl li a').hover(function(){
		var ind=$(this).parent('li').index();
		
		$(this).parent('li').addClass('on').siblings().removeClass('on');
		$('.tabSection .tabBox').hide();		
		$('.tabSection .tabBox:eq('+ind+')').show();
		return false;
	});	
	var islogin = false;
	if (islogin == false) {
		var isShow = 1;
		if(isShow == 1){
			/* base_url = document.location.href.substring(0, document.location.href.indexOf('index.html'), 0); */
		    
			$(".lightbox").lightbox({
			    fitToScreen: true,
			    imageClickClose: false
		    });
			/* $("#dialog_show_img").show(); */
			
		}
		$(function(){$("#lightbox").click();})
	}
});


</script>
<script>
var myArray=new Array();

	myArray["0"] = "https://file.ppbk9.com/img/jxcp/67d7a355-319d-4ce0-a1a3-a64f4650210d.jpg";

	myArray["1"] = "https://file.ppbk9.com/img/jxcp/6ab2fb20-4dd4-4174-9750-81f40774155d.png";

	myArray["2"] = "https://file.ppbk9.com/img/jxcp/f0479b4c-5aef-4c42-8c41-31b91233c7b1.jpg";

	myArray["3"] = "https://file.ppbk9.com/img/jxcp/5d1141ba-1451-4cdd-a78e-c404c07941b4.jpg";

	myArray["4"] = "https://file.ppbk9.com/img/jxcp/b785b223-ad9d-495e-86f8-3dad1ac66f39.png";

$(function(){
	 $(".mainImg .foo").backstretch(myArray, 
		{	fade: 750,
        	duration: 4000
    });
	$(".mainImg .foo").on("backstretch.before", function (e, instance, index) {
		$('.mainImg .foopage a:eq('+index+')').addClass('selected').siblings('a').removeClass('selected');
	});
	$('.mainImg .foopage a').click(function(){
		var index = $(this).index();
		$(".mainImg .foo").backstretch('show',index);
		return false;
	});
	$('.mainImg .prev a').click(function(){
		$(".mainImg .foo").backstretch("prev");
		return false;
	});
	$('.mainImg .next a').click(function(){
		$(".mainImg .foo").backstretch("next");
		return false;
	}); 
	
		//tab切换
	var ind = 0;
	$('.tabSection .tabBox').hide();
	$('.tabSection .tabBox').eq(0).show();

	$('.tabSection .tabUl li a').hover(function(){
		var ind=$(this).parent('li').index();
		$(this).parent('li').addClass('on').siblings().removeClass('on');
		$('.tabSection .tabBox').hide();		
		$('.tabSection .tabBox:eq('+ind+')').show();
		return false;
	});	

});
</script>
