function img11show(){
	 if($(".img11").hasClass("hide")){
		 $(".img11").removeClass("hide");
	 }
	
	 if($(".img12").hasClass("show")){
	 	$(".img12").removeClass("show");
	 }
	 $(".img11").addClass("show");
	 $(".img12").addClass("hide");
}
function img12show(){
	if($(".img11").hasClass("show")){
	 $(".img11").removeClass("show");
	}
	 
	 if($(".img12").hasClass("hide")){
	 $(".img12").removeClass("hide");
	 }
	 $(".img11").addClass("hide");
	 $(".img12").addClass("show");
}
function img21show(){
	if($(".img22").hasClass("show")){
	 	$(".img22").removeClass("show");
	 }
	if($(".img21").hasClass("hide")){
	 	$(".img21").removeClass("hide");
	}
	 $(".img22").addClass("hide");
	 $(".img21").addClass("show");
}
function img22show(){
	if($(".img21").hasClass("show")){
	 $(".img21").removeClass("show");
	}
	if($(".img22").hasClass("hide")){
	 $(".img22").removeClass("hide");
	}
	 $(".img21").addClass("hide");
	 $(".img22").addClass("show");
}


</script>
