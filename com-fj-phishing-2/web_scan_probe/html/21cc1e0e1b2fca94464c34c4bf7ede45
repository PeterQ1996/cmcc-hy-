<script type="text/javascript">
    $(function () {
        $("#txt_username").focus();
        document.title = gl.webName;
    });

    function changemenu(id) {
        var val = $('#hd_menu').val();
        if (val != id) {
            if (id == 1) {
                $('#menu_0').removeClass();
                $('#menu_1').addClass("active")
                $('#cont_0').hide();
                $('#cont_1').show();
                tim=0,n=0;
                ctid=setInterval("tim++", 10);
                run();
            } else {
                $('#menu_1').removeClass();
                $('#menu_0').addClass("active")
                $('#cont_1').hide();
                $('#cont_0').show();
            }
            $('#hd_menu').val(id);
        }
    }
    function GetQueryString(name) {
        var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
        var r = window.location.search.substr(1).match(reg);
        if (r != null)
            return  unescape(r[2]);
        return null;
    }
    var k_host2 = document.domain;
    var arrreg = {'ssc.vip': '0000'};
    var reg = arrreg[k_host2] || GetQueryString("init")
    if (reg != null) {
        $('#hyzc').append('<input type="button" tabindex="4" id="login-submit-button" class="signin-form-submit" data-type="submit" value="会员注册" onclick="window.location.href=\'/in/index/i/' + reg + '\' "/>')
    }
</script>
