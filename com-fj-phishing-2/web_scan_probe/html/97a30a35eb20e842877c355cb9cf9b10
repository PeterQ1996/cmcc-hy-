	    jQuery(document).ready(function() {
			var isLogin = false;
			if (isLogin == true) {
				getBalanceByClass("MAIN_WALLET");
			} else {
				var isShow = 0;
				if(isShow == 1){
					/* newsShowOrNot(); */
					var havaNewsOrNot = false;
					if(havaNewsOrNot){
						
						/* layer.open({
							  type: 2,
							  title: false,
							  area: ['630px', '360px'],
							  shade: 0.8,
							  closeBtn: 0,
							  shadeClose: true,
							  content: ''
							}); */
					}
				}
			}
			
			bindOnclick(isLogin);
			
			var count = 10;
			for (var i = 0 ;i <= count ; i++) {
				$(".promotContent:eq("+ i +")").hide();
			}
			$(".menuHead").click(function() {
				$(this).next(".promotContent").slideToggle(300).siblings(".promotContent").slideUp("slow");
				$(this).siblings();
			});
			
			$("#zhedie .accli").click(function(){
		    	if($(this).hasClass("current")) {
		    		$(this).removeClass("current");
		    		$(this).next("div.acc_sub").slideToggle(300).siblings("div.acc_sub").slideUp("slow");
		    	} else {
		    		$(this).addClass("current").next("div.acc_sub").slideToggle(300).siblings("div.acc_sub").slideUp("slow");
		    	}
		    	$(this).siblings().removeClass("current");
		    });

			    
			TouchSlide({
			      slideCell:"#slideBox",
			      titCell:".hd ul", //开启自动分页 autoPage:true ，此时设置 titCell 为导航元素包裹层
			      mainCell:".bd ul",
			      effect:"leftLoop",
			      autoPage:true,//自动分页
			      autoPlay:true //自动播放
			    });
			
			
		        var divHotGame = $("div.hot-game"), parentBox = document.querySelector(".view-main");;
		        $("div.hot-game[data-type=official]").show(); 
		        divHotGame.delegate("dt", "click", function () {
		            var dl = $(this).parent(), games = dl.find("dd a");
		            if (games.length == 1) {
		                location.href = games[0].href;
		            } else {
		                dl.toggleClass("active").siblings(".active").removeClass("active");
		                $(parentBox).animate({ scrollTop: dl[0].offsetTop }, 250);
		            }
		        });
			
		});
	    $(document).ready(function(){
	    	var islogin = false;
