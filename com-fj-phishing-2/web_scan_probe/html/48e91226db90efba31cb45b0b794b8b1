// 转手机版
function localM(){
   var wgeturl = window.location.href;
   var url = wgeturl.match(/[^\/|^\.]+\.(com|org|me|cc|net)/ig);
   var getdb = wgeturl.match(/\?[^\?]*$/ig);
   if(getdb){
        getdb = '/' + getdb;
   }else{
        getdb = '';
   }
   if(wgeturl.indexOf('https') > 0){
        top.location.href = "https://m." + url + getdb;
   }else{
       top.location.href = "http://m." + url + getdb;
   }
}
//手机版转PC
function localPC(){
    var wgeturl = window.location.href;
    var url = wgeturl.match(/[^\/|^\.]+\.(com|org|me|cc|net)/ig);
    var getdb = wgeturl.match(/\?[^\?]*$/ig);
    if(getdb){
        getdb = '/' + getdb;
    }else{
        getdb = '';
    }
    url = url.replace("m.","www.");
    if(wgeturl.indexOf('https') > 0){
        top.location.href = "https://m." + url + getdb;
    }else{
        top.location.href = "http://m." + url + getdb;
    }
}

if (top.location != location)
{
    top.location.href = location.href;
}

