<script language="JavaScript">
function closeErrors() {
    return true;
}
window.onerror=closeErrors;
var str='欢迎光临【'+'金碧汇彩娱乐城--专注网上娱乐，值得信赖'+'】,请记住我们的网址http://'+'www.jbhc66.com';
