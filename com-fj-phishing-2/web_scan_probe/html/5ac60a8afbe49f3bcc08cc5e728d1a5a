    $(function () {
        var intervals = [];
        var bannerItems = $('.swiper-container .swiper-slide');

        //弹窗初始化
        homeFn.message();
        //只有一张图片时不操作
        if (bannerItems.length > 1) {
            //轮播图
            if (window.Swiper) {
                var $pagination = $('.swiper-pagination');
                var mySwiper = new Swiper('.swiper-container', {
                    pagination: $pagination.length > 0 ? $pagination[0] : undefined,
                    loop: true,
                    grabCursor: true,
                    paginationClickable: true,
                    autoplay: 3000,
                    autoplayDisableOnInteraction: false,
                    speed: 300
                })
                $('#prev_btn').on('click', function (e) {
                    e.preventDefault();
                    mySwiper.swipePrev();
                })
                $('#next_btn').on('click', function (e) {
                    e.preventDefault();
                    mySwiper.swipeNext();
                });
            }
        } else {
            $(".controller,.pagination").hide();
        }
        var gameListTabs = $("#J_subnav_list [data-target]");
        gameListTabs.on("mouseover", function (evt) {
            var evtTarget = $(this);
            evtTarget.addClass("hover").siblings().removeClass("hover");
            var target = $(this).attr("data-target");
            $("#J_subnav_list .JGameList").css("display", "none");
            $("#J_subnav_list " + target).css("display", "block");
        });

        //开奖结果
        ///获取游戏期数信息
        var lotteryResultIds = [
            //游戏缩写---------------官方或信用--游戏名称---------------机选下注--玩法项ID[公共]-生成号码个数-号码范围-选号是否可重复[默认为false]
            { Abbreviation: "ZQSSC", GroupID: 0, GameName: "重庆时时彩", betInfos: { SI: 0, count: 5, range: [0, 9], canRepeat: false } }   //整合
            , { Abbreviation: "BJPKS", GroupID: 0, GameName: "北京PK拾", betInfos: { SI: 1, count: 5, range: [1, 10], canRepeat: false } }   //第1-5名
            //, { Abbreviation: "XYFT", GroupID: 3, GameName: "幸运飞艇", betInfos: { SI: 21294, count: 5, range: [1, 10], canRepeat: false } }   //猜前五复式
            , { Abbreviation: "WMFFC", GroupID: 3, GameName: "极速分分彩", betInfos: { SI: 20953, count: 5, range: [0, 9], canRepeat: false } }   //五星复式
            , { Abbreviation: "DJ1.5FC", GroupID: 3, GameName: "东京1.5分彩", betInfos: { SI: 20953, count: 5, range: [0, 9], canRepeat: false } }   //五星复式
            , { Abbreviation: "TXFFC", GroupID: 0, GameName: "腾讯分分彩", betInfos: { SI: 0, count: 5, range: [0, 9], canRepeat: false } }   //整合
        ];
        homeFn.getGamePeriodsInfo({
            dom: $("#lotteryResultContainer")[0],   //要绑定的元素
            refreshTime: 10,   //数据刷新时间间隔
            gameInfos: lotteryResultIds, //获取开奖结果的游戏id列表
            isRereshMulti: false   //是否刷新多个
        });
        $(window).unload(function () {
            $.each(intervals, function (idx, ele) {
                clearInterval(ele);
            })
        });
    });
