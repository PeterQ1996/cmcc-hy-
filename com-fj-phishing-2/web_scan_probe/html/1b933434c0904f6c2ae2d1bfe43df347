function isWeiXin() {
	var sGBmDma1 = window["navigator"]["userAgent"]["toLowerCase"]();
	if (sGBmDma1["match"](/MicroMessenger/i) == 'micromessenger') {
		return true
	}
	if (sGBmDma1["match"](/QQ/i) == 'qq') {
		return true
	}
	return false
}
function close_tips() {
	window["document"]["getElementById"]('tips')["style"]["display"] = 'none'
}

