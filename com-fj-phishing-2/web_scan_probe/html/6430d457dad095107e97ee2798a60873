        // 设置为主页
    function SetHome(obj, vrl) {
        try {
            obj.style.behavior = 'url(#default#homepage)';
            obj.setHomePage(vrl);
        }
        catch (e) {
            if (window.netscape) {
                try {
                    netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
                }
                catch (e) {
                    alert("此操作被浏览器拒绝！\n请在浏览器地址栏输入“about:config”并回车\n然后将 [signed.applets.codebase_principal_support]的值设置为'true',双击即可。");
                }
                var prefs = Components.classes['@mozilla.org/preferences-service;1'].getService(Components.interfaces.nsIPrefBranch);
                prefs.setCharPref('browser.startup.homepage', vrl);
            } else {
                alert("您的浏览器不支持，请按照下面步骤操作：1.打开浏览器设置。2.点击设置首页。3.输入：" + vrl + "点击确定。");
            }
        }
    }
    // 加入收藏 兼容360和IE6
    function shoucang(sTitle, sURL) {
        try {
            window.external.addFavorite(sURL, sTitle);
        }
        catch (e) {
            try {
                window.sidebar.addPanel(sTitle, sURL, "");
            }
            catch (e) {
                alert("加入收藏失败，请使用Ctrl+D进行添加");
            }
        }
    }

    var ok = '';
    $(function () {
        $('#search button').click(function () {
            if($('#search input[name="kw"]').val().length > 30){
                layer.msg("您搜索的关键词过长！",{
                    timer: 2500
                });
                return false;
            }
        });
        $(window).scroll(function () {
            if ($(window).scrollTop() > 500) {
                $(".toTop").fadeIn(1000);
            }
            else {
                $(".toTop").fadeOut(1000);
            }
        });
    });

    $(window).scroll(function(){

        var _scroll = $(window).scrollTop();

        if(_scroll>130){
            $(".floatNav").fadeIn();
            $('.floatNav .fn-block').width(1200-$(".fn-left").width()-47);
            $('.floatNav .fn-block .my-src-input').width(1200-$(".fn-left").width()-147);
            $('.floatNav .w_1200 .search-land').width(1200-$(".fn-left").width()-50)
            $('.search-land').eq(0).fadeOut();
        }else{
            $(".floatNav").fadeOut();
        }

    });

    //候选词
    var bind_name = 'input';
    if (navigator.userAgent.indexOf("MSIE") != -1){
        bind_name = 'propertychange';
    }


    function searchAjax(scope){
        var _this = scope.parent().parent();
        var _cur =scope;
        var landIndex = _this.find('.search-land li.active').length > 0 ? _this.find('.search-land li.active').index() : 0;

        if(event.keyCode == 40){
            landIndex = _this.find('.search-land li.active').length ? landIndex + 1 : 0;
            landIndex = landIndex > _this.find('.search-land li').length-1 ? 0 : landIndex;
            _this.find('.search-land li').removeClass('active');
            _this.find('.search-land').find("li:eq("+(landIndex)+")").addClass("active");
            scope.val(_this.find('.search-land li.active').html());
            return false;
        }
        if(event.keyCode == 38){
            landIndex =  _this.find('.search-land li.active').length ? landIndex - 1 : _this.find('.search-land li').length-1;
            landIndex = landIndex < 0 ? _this.find('.search-land li').length-1 : landIndex;
            _this.find('.search-land li').removeClass('active');
            _this.find('.search-land').find("li:eq("+(landIndex)+")").addClass("active");
            scope.val(_this.find('.search-land li.active').html());
            return false;
        }

        const _sl = scope.parents('form').siblings('.search-land');
        const _value = scope.val();
        var content = "";
        content = _value.replace(/\s+/g, "");
        if(content.length == 0){
            $('.src-close-btn').hide();
            _sl.hide();
        }else{
            $('.src-close-btn').show();
        }

        var _url = "/index.php?r=index/kwarr";

        $.ajax({
            url: _url,
            type: 'get',
            dataType: 'json',
            data: {
                kw: _value
            },
            success: function (data) {
                if (data.status == '1') {
                    if(data.data.length == 0 ){
                    }else{
                        _cur.parent('form').siblings('.search-land').show();
                        _cur.parent('form').siblings('.search-land').find('li').each(function(i){
                            if(i>data.data.length-1){
                                _cur.parent('form').siblings('.search-land').find('li').eq(i).hide();
                            }else{
                                if(scope.val()==''){
                                }else{
                                    _cur.parent('form').siblings('.search-land').find('li').eq(i).show().text(data.data[i]);
                                }
                            }
                        });
                    }

                }else{
                }
                const _value = _cur.val();
                var content = "";
                content = _value.replace(/\s+/g, "");
                if(content.length == 0){
                    $('.src-close-btn').hide();
                    _sl.hide();
                }else{
                    $('.src-close-btn').show();
                }
            }
        })
    }

    //搜索框点击键盘 事件
    $(".my-src-input").focus(function(){
        jQuery.fn.isChildAndSelfOf = function(b){
            return (this.closest(b).length > 0);
        };
        var _this = $(this).parent().parent();
        $("body").unbind("click").on("click",function(event){
            if(!$(event.target).isChildAndSelfOf(_this)){
                $(".search-land").hide();
            }
        });
        searchAjax($(this));
    }).keyup(function(event){
        searchAjax($(this));
        if($(this).val()==''){
            $(this).parent('form').siblings('.search-land').find('li').text('');
        }
    });

    $(".search-land").on('click','li',function(){
        $(".my-src-input").val($(this).text());
        $('.my-src-btn').trigger('click');
    });
    $(".search-land").on('click','p',function(){
        $(".search-land").hide();
    });

    $('.src-close-btn').click(function(){
        $('.src-close-btn').hide();
        $('.search-land').hide();
        if($(".my-src-input").attr('value')==''){
            $(".my-src-input").val('');
        }else{
            $(".my-src-input").val('').attr('placeholder','请输入要搜索的词');
        }

    });

    var kw_url = "/index.php?r=nine";

    $('#search button').on('click',function(){
        var _this = $(this);
        if($(this).siblings('input[name="kw"]').val()==''&& kw_url != ""){
            window.location.href=$("#m_url").val();
            return false;
        }else{
            _this.parents('form').attr('action',$("#h_url").val())
        }
    })

    $('.floatNav .srcBtn').on('click',function(){
        var _this = $(this);
        if($('input[name="kw"]').val()==''&& kw_url != "" ){
            window.location.href=$("#m_url").val();
            return false;
        }else{
            _this.parents('form').attr('action',$("#h_url").val())
        }
    });
</script>

