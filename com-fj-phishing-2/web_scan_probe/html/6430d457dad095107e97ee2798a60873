<script type="text/javascript">

    
    //xc statistics
    ;!(function(window, document) {
        "use strict"
        try {
            var space = "xctj";

            // 创建队列
            window["xckj"] = space;
            window[space] = window[space] || function () {
                (window[space].list = window[space].list || new Array()).push(arguments)
            };
            window[space].time = window[space].time || 1 * new Date();

            // 创建元素
            var script = document.createElement("script");

            // 设置元素
            script.type = "text/javascript";
            script.src = "https://tj.ffquan.com/satc.js?z=20180530" ; /* 配置satc.js路径 */
            script.async = 1;

            // 添加元素
            var node = document.getElementsByTagName("script")[0];
            node.parentNode.insertBefore(script, node)
        } catch (error) { }
    })(window, document);

