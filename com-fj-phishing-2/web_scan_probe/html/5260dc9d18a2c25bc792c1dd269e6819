						function getLoginedInfo() 
						{
							$.post("/?r=mobile/index/json",{},
							   function(res){
									if(res.notice == '')
									{
										$("#noticeId").html("<c>公告</c>");
									}
									else
									{
								
								$("#noticeId").html("<c>公告</c><marquee class='marquee' direction='left' behavior='scroll' scrollamount='3'>"+res.notice+"</marquee>");
									}
									
									if(res.name !== ''){
										$("#user2").hide();
										$("#user3").show();
										$("#user1").hide();
										$("#user4").show();
										$("#user").show();
										$("#h_menber").html(res.name);
										$("#m").html(res.name+",余额:"+res.money);
									} 
									if(res.msg !== ''){
										alert(res.msg);
										window.location.href = "/aomen/index.html";
									}
							   },"json");
						 }
						</script>
