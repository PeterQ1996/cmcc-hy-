<script type="text/javascript">
		getPop();
		function getPop(){
		   $.post("/?r=mobile/index/pop",{},
		   function(res){
				if(res.is_pop_msg_wap == '1' && res.index_pop_msg_wap!=null && res.index_pop_msg_wap.replace(/\s+/g,"")!="")
				{
					layer.open({
						content: res.index_pop_msg_wap.replace(/\\/g,""),
						btn: '我知道了'
					});
				}
				if(res.is_pop_msg_hb == '1')
				{
					$("#popHbImg").attr('src','http://'+res.conf_www + res.index_pop_pic_url_hb);
					$("#hbAddrUrl").attr('href','http://'+res.conf_www_hb);
					$("#popHbDiv").css({"display":""});
				}
		   },"json");
		}	    
		function closeHbDiv(){
			$("#popHbDiv").css({"display":"none"});
		}
</script>
