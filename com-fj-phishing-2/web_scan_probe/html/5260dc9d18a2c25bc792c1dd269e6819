	    <script type="text/javascript">
	    
	    var collect = {
    		"cookieName":"std",
    		"showCollect":function(){
    			var $this = this;
    			if (!(/ipad|iphone|mac/i.test(navigator.userAgent))){
    				return;
    			}
    			var value = doCookie($this.cookieName);
    			if(value == null && navigator.userAgent.indexOf('Safari') > -1 ){
    				$("#desktop").css({"display":"block"});
    			}
    		},
    		"hiddCollect":function(){
    			var $this = this;	
    			$("#desktop").css({"display":"none"});
    			doCookie($this.cookieName,null,1,1);
    		}
    	};
	    
	    function doCookie(cN,cD,cV,cL) {
	    	if(cV) {
	    		if(cL) {
	    			var d=new Date();
	    			d.setTime(d.getTime()+(cL*24*60*60*1000));
	    			var b="; expires="+d.toGMTString()
	    		} else {
	    			var b=""
	    		}
	    		document.cookie=cN+"="+cV+b+"; path=/;"+(cD?" domain="+cD+";":"")
	    	} else {
	    		var e=cN+"=";
	    		var s=document.cookie.split(";");
	    		for(var g=0;g<s.length;g++) {
	    			var h=s[g];
	    			while(h.charAt(0)==" ") {h=h.substring(1,h.length)}
	    			if(h.indexOf(e)==0) {return h.substring(e.length,h.length)}
	    		}
	    		return null
	    	}
	    }
	    
	    $(function(){
	    	collect.showCollect();
	    	
	    	$(".del").click(function(){
	    		collect.hiddCollect();
	    	});
	    });
	    
	    </script>
