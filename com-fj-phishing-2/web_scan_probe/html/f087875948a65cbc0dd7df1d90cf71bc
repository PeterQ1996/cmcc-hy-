		  function shake(element,className,times){
		      var i = 0,
		          t = false ,
		          o = element.attr("class"),
		          c = "",
		          times = times||2;
		      if(t) return;
		      t = setInterval(function(){
		        i++;
		        c = i%2 ? o + ' ' + className : o;
		        element.attr("class",c);
		
		        if(i==2*times){
		          clearInterval(t);
		          element.removeClass(className);
		          }
		        },400);
		      };
		      
		  $(function(){
		    //domready 就闪动
		    shake($(".box"),"resd",-1);
		    });   

		  var demoGame = false;
		  if(demoGame == true){
			$(function(){$("#demoPlay").click();})
		  }

	});
