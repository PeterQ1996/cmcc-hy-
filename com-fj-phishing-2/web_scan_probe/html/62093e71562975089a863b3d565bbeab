<script type="text/javascript">
    function rnd(n, m){
        var random = Math.floor(Math.random()*(m-n+1)+n);
        return random;
    }
    $(function () {
        $('.short').click( function(){
            var amount = $(this).attr('val');
            $('input[name="coin"]').val( amount );
            return false;
        });
        var time = getNowFormatDate();
        $('#P_Time').val(time);
    })

    function btnOK_zf_onclick() {
        var username = $("#username").val();
        var coin = $("#coin").val();
        var PerSonGuid=$("#PerSonGuid").val();
        var bankck = $('input[name="radzhif_code"]:checked').val();        
        var rusername = $("#rusername").val();
        var VerCode = $("#VerCode").val();
        if (username == null || username == "") {
            alert("[提示]用户名不能为空！");
            $("#username").focus();
            return false;
        }
        if (rusername == null || rusername == "" || rusername != username) {
            alert("[提示]2次用户输入不一致！");
            $("#rusername").focus();
            return false;
        }

        
        if (bankck == null || bankck == "") {
            alert("[提示]支付类型不能为空！");
            return false;
        }
        if (bankck == "3")
        {
            var banklist = $('input[name="radbank"]:checked').val();
            if (banklist == null || banklist == "")
            {
                alert("[提示]请选择支付银行！");
                return false;
            }
        }
        if (coin == null || coin == "") {
            alert("[提示]请填写存款金额！");
            $("#coin").focus();
            return false;
        }

        if (isNaN(coin)) {
            alert("[提示]存款额度非有效数字！");
            $("#coin").focus();
            return false;
        }
        if (coin < parseFloat(payLimit_low)) {
            alert("[提示]" + payLimit_low + "元以上或者" + payLimit_low + "元才能存款！");
            $("#coin").focus();
            return false;
        }

        if (coin > parseFloat(payLimit_high) && parseFloat(payLimit_high)>0) {
            alert("[提示]存款金额不能超过" + payLimit_high + "！");
            $("#coin").focus();
            return false;
        }
                
    $("#pay").submit();
    return true;
    }
    

</script>

