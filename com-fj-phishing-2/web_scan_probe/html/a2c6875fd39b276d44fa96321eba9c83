<script src="https://iiimgzy12qt.yimeitaotaoci.com//Scripts/Common/swiper2.7.6/js/idangerous.swiper.min.js?2073ab22ec"></script>
<script>
    var serverTime = new Date('2018/08/08 13:00:44') - 0;// fff
    var localTime = new Date() - 0;
    $(function () {
        $('.hot_game_toggle a').click(function () {
            var index = $(this).index();
            $('.hotgame_con>div').eq(index).show().siblings().hide();
            $(this).addClass('active').siblings().removeClass('active')
        });

        //开奖结果
        ///获取游戏期数信息
        var lotteryResultIds = [
            //游戏缩写---------------官方或信用--游戏名称---------------机选下注--玩法项ID[公共]-生成号码个数-号码范围-选号是否可重复[默认为false]
            { Abbreviation: "ZQSSC", GroupID: 0, GameName: "重庆时时彩", betInfos: { SI: 0, count: 5, range: [0, 9], canRepeat: false } }   //整合
            , { Abbreviation: "BJPKS", GroupID: 0, GameName: "北京PK拾", betInfos: { SI: 1, count: 5, range: [1, 10], canRepeat: false } }   //第1-5名
            , { Abbreviation: "GXK3", GroupID: 0, GameName: "广西快3", betInfos: { SI: 0, count: 3, range: [1, 6] }, canRepeat: false }   //大小骰宝
            , { Abbreviation: "PLSW", GroupID: 3, GameName: "排列三、五", betInfos: { SI: 20985, count: 3, range: [0, 9], canRepeat: false } }   //前三直选复式
            , { Abbreviation: "⑥HC", GroupID: 0, GameName: "六合彩", betInfos: { SI: 0, count: 7, range: [1, 40] }, canRepeat: false }  //特码
        ];
        homeFn.getGamePeriodsInfo({
            dom: $("#lotteryResultContainer")[0],   //要绑定的元素
            refreshTime: 10,   //数据刷新时间间隔
            gameInfos: lotteryResultIds, //获取开奖结果的游戏id列表
            isRereshMulti: false   //是否刷新多个
        });

        var bannerItems = $('.swiper-container .swiper-slide');
        //只有一张图片时不操作
        if (bannerItems.length > 1) {
            //轮播图
            if (window.Swiper) {
                var mySwiper = new Swiper('.swiper-container', {
                    //pagination: '.swiper-pagination',
                    loop: true,
                    grabCursor: true,
                    paginationClickable: true,
                    autoplay: 3000,
                    autoplayDisableOnInteraction: false,
                    speed: 300
                })
                $('#prev_btn').on('click', function (e) {
                    e.preventDefault();
                    mySwiper.swipePrev();
                })
                $('#next_btn').on('click', function (e) {
                    e.preventDefault();
                    mySwiper.swipeNext();
                });
            }
        } else {
            $(".controller,.swiper-pagination").hide();
        }
    });
</script>

