    <script>
        //关闭对联
        $(".dlc").on("click", function () {
            $(this).parent().css("display", "none");
        });

        var amb = new Object();
        /*初始化圈动画度数*/
        amb.tba = $(".Turntable>a").attr("tfd", "0");
        //设置速度
        amb.tbas = 20;
        /*圈动画*/
        setInterval(function () {
            //获取当前度数
            var ctd = parseInt($(amb.tba).attr("tfd"));
            ctd++;
            //初始化旋转度数
            if (ctd >= 359) {
                ctd = 0;
                $(amb.tba).attr("tfd", ctd)
            }

            //设置度数
            $(amb.tba).css(
                { "transform": "rotate(" + ctd + "deg)" }
            );

            //保存度数
            $(amb.tba).attr("tfd", ctd);
        }, amb.tbas);
    </script>
