    <script>
        $(function () {
            var kefuw = $(".J-kefuw-fixed");
            var kefuw0 = kefuw.eq(0);
            var height = kefuw0.height();

            $(window).scroll(function (evt) {
                var top = $(document).scrollTop();
                if (kefuw0.position().top + height < top - 40) {
                    kefuw.css("top", top - 40 + "px");
                }

                kefuw.stop().animate({ top: 292 + top + "px" }, 500, "linear");
            });

            ///文字闪动
            colorSwitch('#ColorSwitch', 300);
            function colorSwitch(rootEle, timeout, loop) {
                var colorSwitch = $('[data-color]', $(rootEle));
                var switchInfos = $.map(colorSwitch, function (ele) {
                    var colors = $(ele).attr('data-color').split(',');
                    return {
                        ele: $(ele),
                        colors: colors,
                        lens: colors.length,
                        cur: 0
                    };
                });
                var loopTime = 0;
                function loopsSwitch() {
                    if (!loop || loop < 0 || loopTime < loop) {
                        $.each(switchInfos, function (idx, info) {
                            info.ele.css('color', info.colors[info.cur]);
                            info.cur = (info.cur + 1) % info.lens;
                        });
                        setTimeout(loopsSwitch, timeout)
                    }
                    loopTime++;
                }
                loopsSwitch();
            }
        })
    </script>

