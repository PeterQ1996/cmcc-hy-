<script type="text/javascript">
var iLogin = 2;
$(function(){
	if(iLogin == 2){return false;}
	$.ajax({
		url:'/center/news/message/messageCount.do',
		success:function(res){
			if(res.messageCount > 0){
				$(".message_ts_p").text(res.messageCount).removeClass('hidn');
				$("#unread_num_new").removeClass("icon");
			}else{
				$(".message_ts_p").addClass("hidn");
				$("#unread_num_new").addClass("icon");
			}
		}
	})
})
</script>
