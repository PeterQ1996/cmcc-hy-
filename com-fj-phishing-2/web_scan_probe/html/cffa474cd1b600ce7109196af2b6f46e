	<script type="text/javascript">
		var iL = $("#isLogin").val();
		var isJoint = false;
		$(function() {
			initLunBo();
			initGg();
			$('.myScroll').myScroll({
				speed: 40, //数值越大，速度越慢
				rowHeight: 25 //li的高度
			});
			if (!isJoint) {Base.getLotJointData();}
			$(".notice-tab li.tab_g").mouseover(
					function() {
						$(this).addClass("on").siblings().removeClass("on");
						var i = $(this).index();
						$(".notice-main").find('.draw-contents:eq(' + i + ')')
								.show().siblings().hide();
					})

			//自助服务
			$(".help-tab li").mouseover(function() {
				$(this).addClass("on").siblings().removeClass("on");
				var $class = $(this).find('a').attr("class");
				$(this).parent().siblings('ul').hide();
				$(this).parent().siblings('.' + $class + '-box').show();
			})
			
			
			$("#scroll").click(function(){
				window.open("/lotteryV3/gg.do","gg");
			})
		})

		function initLunBo() {
			$.ajax({
				url : '/getConfig/getLunBo.do',
				success : function(data) {
				if (data.length > 0) {
					var temp = "", i = 0;
					$.each(
						data,
						function(index, item) {
							i++;
							temp += '<li><a href="'+item.titleUrl+'"><img src="'+item.titleImg+'" /></a></li>'
						})
						$("#Slide .slidebox").html(temp);
						//加载图片完成启动轮播定时器
						if (i > 1) {
							startQuartz();
						}
					}
				}
			})
		}

		function startQuartz() {
			$("#Slide").slide({
				affect : 4, //切换状态1-4
				time : 5000,
				speed : 1000,
				//dot_text:false,
			});
		}

		function initGg() {
			// 最新消息跑馬燈
			$.ajax({
				url : "/getConfig/getArticle.do",
				data : {
					code : 13
				},
				type : "post",
				dataType : 'json',
				success : function(r) {
					var col = '';
					for (var i = 0; i < r.length; i++) {
						col += r[i].content + "&nbsp;&nbsp;&nbsp;";
					}
					$("#scroll ul li").html(col);
					$("#scroll").marquee({  
				        direction:"left",  
				        speed:30  
				    }); 
				}
			});
		}
		
	</script>
	

<script>
var param = 'index';
$(function(){
	$.ajax({
		url : "/getConfig/getArticle.do",
		data:{code:14},
		type : "post",
		dataType : 'json',
		success : function(j) {
			if(j.length>0){
				switch(param){
				case "index":
					if(j[0].index){
						dialogGg(j);
					}
					break;
				case "register":
					if(j[0].reg){
						dialogGg(j);
					}
					break;
				case "agentRegister":
					if(j[0].mutil){
						dialogGg(j);
					}
					break;
				case "cpBuy":
					if(j[0].cp){
						dialogGg(j);
					}
					break;
				}
			}
			
		}
	});
});

function dialogGg(j){
	var stc = $('#tan_chuang_two').val();
	if(stc){
		dialogGg2(j);
	}else{
		dialogGg1(j);
	}
}

function dialogGg1(j){
	var title="";
	var content="";
	var fw = 550;
	var fh = 550;
	if(j[0].frameWidth){
		fw = j[0].frameWidth;
	}
	if(j[0].frameHeight){
		fh = j[0].frameHeight;
	}
	title = j[0].title;
	if(!title){
		title = " ";
		temp = 1;
	}
	layer.open({
		  type: 1, //page层
		  area: [fw+'px', fh+'px'],
		  title: title,
		  shade: 0.6, //遮罩透明度
		  skin:'layui-layer-rim',
		  scrollbar: false,
		  offset: '16%',//距离顶部
		  moveType: 0, //拖拽风格，0是默认，1是传统拖动
		  shift: 1, //0-6的动画形式，-1不开启
		  btn: ['知道了'],
		  content: '<div style="overflow-x: hidden;overflow-y:hidden;height:100%;">'+j[0].content+'</div>'
	}); 
}

function dialogGg2(j){
 var layerIndex;
   if(j && j.length>0){
    layer.closeAll(layerIndex);
    var d = {list:j};
    var notice_html = htmlHelp(j);
	var fw = 550;
	var fh = 550;
	if(j[0].frameWidth){
		fw = j[0].frameWidth;
	}
	if(j[0].frameHeight){
		fh = j[0].frameHeight;
	}
    layerIndex = layer.open({
       type: 1,
       shade: .5,
       scrollbar: false,
       offset: '16%',//距离顶部
       title: false, //不显示标题
       moveType: 1,
       area: [fw + 'px', fh + 'px'],
       content: notice_html
    });
    var _notice = $(".noticeDialog");
    _notice.find(".left-nav").on("click","ul li",function(){
     var _this = $(this), index = _this.data("index") , id = _this.data("id");
     _this.addClass("active").siblings("li").removeClass("active");
     _this.parent().siblings(".content-border").find("#line").attr("class","line"+index);
     _notice.find(".right-content .notice_index_" + index).addClass("active").siblings().removeClass("active");
     _notice.find(".right-content .notice_index_" + index).show().siblings().hide();
    })
     $('.right-content .notice_content').css('overflow','auto');
     $('.layui-layer .layui-layer-setwin a').css('transition','transform 0.5s cubic-bezier(.16,.81,.32,1), opacity 0.5s ease');
     $('.layui-layer .layui-layer-setwin a').css('width','30px');
     $('.layui-layer .layui-layer-setwin a').css('height','30px');
   }
}

function htmlHelp(j){
	var html = '<div class="noticeDialogTitle"><span>最新公告</span></div>';
	    html += '<div class="noticeDialog"><div class="left-nav"><ul>';
	    $.each(j,function(i,k){
	    	html += '<li data-index="'+i+'" data-id="'+k.id+'" class="choose';
	    	if(i == 0){
	    		html += ' active';
	    	}
	    	html += '"><div class="icon"><i class="fa fa-clone"></i></div><span>'+k.title+'</span></li>';
	    })
	    html += '</ul><div class="content-border"><div class="line0"></div></div></div><div class="right-content">';
	    $.each(j,function(i,k){
	    	html += '<div class="notice_index_'+i+' notice_content ';
	    	if(i == 0){
	    		html += 'active" ';
	    		html += 'style="display:block"';
			}else{
				html += '" style="display:none"';
			}
	    	html += '><p>'+k.content+'</p></div>';
	    })
	    html += '</div></div>';
	    return html;
}
</script>
	

