		//根字号
		function fsrem(){
			document.getElementsByTagName("html")[0].style.fontSize = document.getElementById("wrap").offsetWidth/100+"px";
		}
		fsrem();
		window.onresize = function(){
			fsrem();
		}
		//倒计时
		function show_retime(){
			window.setTimeout("show_retime()", 1); 
			var ObjDay=new Date("2100-12-31 00:00:00".replace(/-/g,'/'));//设置时间以24小时倒计时循环，可忽略日期
			today=new Date(); 
			timeold=(ObjDay.getTime()-today.getTime()); 
			sectimeold=timeold/1000 
			secondsold=Math.floor(sectimeold); 
			msPerDay=24*60*60*1000 
			e_daysold=timeold/msPerDay 
			daysold=Math.floor(e_daysold); 
			e_hrsold=(e_daysold-daysold)*24; 
			hrsold=Math.floor(e_hrsold); 
			e_minsold=(e_hrsold-hrsold)*60;
			minsold=Math.floor((e_hrsold-hrsold)*60); 
			e_seconds = (e_minsold-minsold)*60;
			seconds=Math.floor((e_minsold-minsold)*60); 
			ms = e_seconds-seconds;
			ms = new String(ms)
			ms = ms.substr(2,2);
			document.getElementById("timer").innerHTML = "<span class='n n1'>"+(hrsold<10?'0'+hrsold:hrsold)+"</span><span class='n n2'>"+(minsold<10?'0'+minsold:minsold)+"</span><span class='n n3'>"+(seconds<10?'0'+seconds:seconds)+"</span><span class='n n4'>"+ms+"</span>";
		} 
		show_retime();
	</script>
