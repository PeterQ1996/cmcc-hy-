<script type="text/javascript">
var iLogin = 2;APP
$(function(){
	if(iLogin == 2){return false;}
	$.ajax({
		url:'/center/news/message/messageCount.do',
		success:function(res){
			if(res.messageCount > 0){
				$(".message_ts_p").text(res.messageCount).removeClass('hidn');
				$("#unread_num_new").removeClass("icon");
			}else{
				$(".message_ts_p").addClass("hidn");
				$("#unread_num_new").addClass("icon");
			}
		}
	})
})
//在线客服
///lotteryV3/service.do
function zxkf(){
	var open = window.open();
	$.ajax({
		url:"/memInfo/common/siteInfo/base.do",
		dataType:"json",
		success:function(j){
			if(j.success){
				var url = j.item;
				if(url.indexOf('http://')==-1 && url.indexOf('https://')==-1){
					url = 'http://' + url;
				}
				open.location = url;
			}else{
				$('#zxkf').attr('href','#').removeAttr('target');
			}
		}
	});
}
</script>
