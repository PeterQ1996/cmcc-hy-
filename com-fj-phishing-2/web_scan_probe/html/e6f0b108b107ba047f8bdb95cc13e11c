<script type="text/javascript">
    getLatestReward();
    setInterval(function () {
        getLatestReward();
    }, 3000);

    function getLatestReward() {
        var url = '/Home/GetLatestReward';
        $.ajax({
            url: url,
            method: 'post',
            success: function (data) {
                $('#js-scroll-content').html('');
                for (var i = 0; i < data.length; i++) {
                    var userName = data[i].UserName.substr(0, 3) + '...';
                    var money = data[i].Money;
                    var time = convertToLocalDate(data[i].CreateTime);

                    //<li><span class="first">恭喜 zyo...</span><span class="w150"> 获得 <b>22.24元</b> 红包</span><span class="w80"><small>2018-04-23 05:53:03</small></span></li>
                    var html = '<li><span class="first">恭喜 ' + userName + '</span><span class="w150"> 获得 <b>' + money + '元</b> 红包</span><span class="w80"><small>' + time + '</small></span></li>';
                    $('#js-scroll-content').append(html);
                }
            }
        });
    }
</script>

    

